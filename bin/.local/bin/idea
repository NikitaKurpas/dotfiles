#!/bin/bash
#Generated by JetBrains Toolbox 1.22.10970 at 2022-04-08T14:09:06.790308

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

app_dir="$HOME/Library/Application Support/JetBrains/Toolbox/apps/IDEA-U"
version=$(ls "$app_dir/ch-0" | sort --reverse | head -1)

open -na "$app_dir/ch-0/$version/IntelliJ IDEA.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"